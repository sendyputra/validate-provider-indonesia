{
  "name": "validate-provider-indonesia",
  "version": "1.0.0",
  "description": "Validate Mobile Phone Number in Indonesia",
  "main": "dist/validate-provider-indonesia.cjs.js",
  "module": "dist/validate-provider-indonesia.esm.js",
  "browser": "dist/validate-provider-indonesia.umd.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "rollup -c -w",
    "build": "rollup -c",
    "build:types": "rm -rf build && tsc -p ./tsconfig.json --outDir build --declaration true && api-extractor run",
    "docs": "typedoc src --out docs --tsconfig tsconfig.json",
    "test": "jest --verbose",
    "lint": "eslint --fix main.js",
    "format": "prettier -w src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sendyputra/validate-provider-indonesia.git"
  },
  "keywords": ["no telp","indonesia", "provider indonesia","no hp"],
  "author": "sendypw",
  "bugs": {
    "url": "https://github.com/sendyputra/validate-provider-indonesia/issues"
  },
  "homepage": "https://github.com/sendyputra/validate-provider-indonesia",
  "license": "ISC",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.3",
    "@types/jest": "^28.1.4",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "eslint": "^8.18.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^28.1.2",
    "prettier": "^2.7.1",
    "rollup": "^2.75.7",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@microsoft/api-extractor": "^7.28.1",
    "typedoc": "^0.23.2"
  }
}
